<ServerManagerConfiguration>
  <ProxyGroup name="filters">
   <SourceProxy name="FourFlowGraph" class="vtkFourFlowGraph" label="Graph">
     <InputProperty
        name="Input"
        command="AddInputConnection"
        clean_command="RemoveAllInputs"
        multiple_input="0">
       <ProxyGroupDomain name="groups">
         <Group name="sources"/>
         <Group name="filters"/>
       </ProxyGroupDomain>
       <DataTypeDomain name="input_type" composite_data_supported="1">
         <DataType value="vtkDataObject"/>
       </DataTypeDomain>
       <InputArrayDomain name="input_array" attribute_type="point"
                         number_of_components="3"/>
     </InputProperty>

      <InputProperty
         name="Source"
         command="AddSourceConnection"
         clean_command="RemoveAllSources"
         multiple_input="0">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkDataSet"/>
        </DataTypeDomain>
      </InputProperty>


     <IntVectorProperty
        name="ShowFlow"
        command="SetShowFlow"
        default_values="1"
        number_of_elements="1"
        information_property="ShowFlowInfo">
        <BooleanDomain name="bool" />
     </IntVectorProperty>

     <IntVectorProperty
        name="ShowFlowInfo"
        command="GetShowFlow"
        information_only="1">
        <SimpleIntInformationHelper/>
     </IntVectorProperty>

     <IntVectorProperty
        name="ShowMaximumVelocity"
        command="SetShowMaximumVelocity"
        default_values="0"
        number_of_elements="1"
        information_property="ShowMaximumVelocityInfo">
        <BooleanDomain name="bool" />
     </IntVectorProperty>

     <IntVectorProperty
        name="ShowMaximumVelocityInfo"
        command="GetShowMaximumVelocity"
        information_only="1">
        <SimpleIntInformationHelper/>
     </IntVectorProperty>


     <IntVectorProperty
        name="ShowMeanVelocity"
        command="SetShowMeanVelocity"
        default_values="0"
        number_of_elements="1"
        information_property="ShowMeanVelocityInfo">
        <BooleanDomain name="bool" />
     </IntVectorProperty>

     <IntVectorProperty
        name="ShowMeanVelocityInfo"
        command="GetShowMeanVelocity"
        information_only="1">
        <SimpleIntInformationHelper/>
     </IntVectorProperty>

     <Hints>
       <Visibility replace_input="0" />
        <!-- View can be used to specify the preferred view for the proxy -->
        <View type="XYChartView" />
        <Plotable />
     </Hints>
   <!-- End ExtractGrid -->
   </SourceProxy>  
  </ProxyGroup>
</ServerManagerConfiguration>
